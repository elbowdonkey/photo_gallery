= form_for(resource, :as => resource_name, :url => unlock_path(resource_name), :html => {:class => "form-signin"}) do |f|
  %h2.form-signin-heading Resend unlock instructions
  = devise_error_messages!
  %div
    = f.label :email
    %br/
    = f.email_field :email, :autofocus => true
  %div= f.submit "Resend unlock instructions"